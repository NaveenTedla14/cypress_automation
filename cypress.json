{
  "reporter": "cypress-multi-reporters",
  "reporterOptions": {
    "reporterEnabled": "mochawesome",
    "mochawesomeReporterOptions": {
      "reportDir": "cypress/reports/mocha",
      "quite": true,
      "overwrite": false,
      "html": false,
      "json": true
    }
  },
  "env" : {
    "url" : "https://demoqa.com"
  },
  "retries": 1,
  "defaultCommandTimeout": 20000,
  "video": false,
  "viewportWidth": 1700,
  "viewportHeight": 1200,
  "compilerOptions": {
    "types": ["cypress"]
  }
}
